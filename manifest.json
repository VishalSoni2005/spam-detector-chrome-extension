{
  "manifest_version": 3,
  "name": "Spam Detector",
  "version": "1.0",
  "description": "Detect spam in Gmail",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["http://localhost:8000/*"],
  "content_scripts": [
    {
      "matches": ["https://mail.google.com/*"],
      "js": ["content.js"]
    }
  ]
}
